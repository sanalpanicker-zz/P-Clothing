{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "globals": {
      "__DEV__": false,
      "__PROD__": false,
      "__DEBUG__": false
    },
    "rules": {
      "spaced-comment": 0,
      "react/require-default-props": 0,
      "react/destructuring-assignment": 0,
      "indent": [
        "error",
        2,
        {
          "MemberExpression": 0,
          "SwitchCase": 1
        }
      ],
      "comma-dangle": [
        1,
        "never"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ],
      "arrow-spacing": [
        2,
        {
          "before": true,
          "after": true
        }
      ],
      "react/jsx-boolean-value": [
        2,
        "always"
      ],
      "arrow-body-style": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "react/jsx-filename-extension": 0,
      "react/forbid-prop-types": 0,
      "no-underscore-dangle": 0,
      "arrow-parens": [
        2,
        "always"
      ],
      "max-len": [
        2,
        120
      ],
      "import/no-named-as-default": 0,
      "no-console": 0
    }
  }
  